    <Button
        android:id="@+id/actualizar_registro_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="7"
        android:layout_column="4"
        android:text="ACTUALIZAR" />
</GridLayout>