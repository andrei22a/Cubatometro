        android:id="@+id/litros_quitar_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="1"
        android:src="@android:drawable/ic_menu_close_clear_cancel" />

